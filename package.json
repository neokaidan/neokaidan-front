{
  "name": "neokaidan-front",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neokaidan/neokaidan-front.git"
  },
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "prettier": "prettier --config .prettierrc --write {public,src}/**/*.{tsx,ts,jsx,js,html,scss}",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js src/ --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-loader": "^9.1.2",
    "commitlint": "^17.4.4",
    "css-loader": "^6.7.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g .commitlintrc",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{public,src}/**/*.{tsx,ts,jsx,js,html,scss}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
